{
  "name": "editor",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "analyze": "next experimental-analyze",
    "build": "next build",
    "build:e2e": "env $(cat ../../packages/db/.env.e2e | xargs) next build",
    "dev": "next dev -p 3003",
    "e2e": "env $(cat ../../packages/db/.env.e2e | xargs) playwright test",
    "e2e:ui": "env $(cat ../../packages/db/.env.e2e | xargs) playwright test --ui",
    "start": "next start -p 3003",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "next typegen && tsgo --noEmit"
  },
  "dependencies": {
    "@dnd-kit/core": "6.3.1",
    "@dnd-kit/sortable": "10.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@formatjs/intl-localematcher": "0.7.3",
    "@zoonk/core": "workspace:*",
    "@zoonk/db": "workspace:*",
    "@zoonk/error-reporter": "workspace:*",
    "@zoonk/next": "workspace:*",
    "@zoonk/ui": "workspace:*",
    "@zoonk/utils": "workspace:*",
    "lucide-react": "0.562.0",
    "negotiator": "1.0.0",
    "next": "16.1.5",
    "next-intl": "4.8.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "@types/negotiator": "0.6.4",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/tmp": "0.2.6",
    "@typescript/native-preview": "7.0.0-dev.20260127.1",
    "@zoonk/e2e": "workspace:*",
    "@zoonk/testing": "workspace:*",
    "@zoonk/tsconfig": "workspace:*",
    "babel-plugin-react-compiler": "1.0.0",
    "tmp": "0.2.5",
    "typescript": "^5",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  }
